{
    "name": "fullscreeninteractive/silverstripe-dropdownimagefield",
    "description": "Dropdown field with image support for SilverStripe CMS",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "dropdown", "image", "field"],
    "license": "MIT",
    "support": {
        "issues": "http://github.com/fullscreeninteractive/silverstripe-dropdownimagefield/issues"
    },
    "require": {
        "silverstripe/framework": "^6"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        },
        "expose": [
            "css",
            "javascript"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
